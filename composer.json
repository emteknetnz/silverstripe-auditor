{
	"name": "silverstripe/auditor",
	"description": "Adds security audit trail to SilverStripe.",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "audit"],
	"license": "BSD-3-Clause",
	"require": {
		"silverstripe/framework": "~3.7",
		"silverstripe/cms": "~3.7",
		"monolog/monolog": "~1.11",
		"psr/log": "~1.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "^5.7"
	},
  "autoload": {
		"psr-4": {
			"SilverStripe\\Auditor\\": "code"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SilverStripe\\Auditor\\Tests": "tests"
		}
	},
    "extra": {
        "branch-alias": {
            "1.x-dev": "1.1.x-dev"
        }
    }
}
